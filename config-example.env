# Configurações do Next.js
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here

# Configurações de Banco de Dados (se necessário)
# DATABASE_URL="postgresql://username:password@localhost:5432/database_name"

# Configurações de Autenticação
# GOOGLE_CLIENT_ID=your-google-client-id
# GOOGLE_CLIENT_SECRET=your-google-client-secret

# Configurações de API (se necessário)
# API_BASE_URL=http://localhost:3000/api

# APIs dos Jogos
# Riot Games API (Valorant, League of Legends, TFT)
NEXT_PUBLIC_RIOT_API_KEY=your-riot-api-key-here

# Steam API (Counter-Strike 2)
NEXT_PUBLIC_STEAM_API_KEY=your-steam-api-key-here

# Configurações de Rate Limiting
RIOT_API_RATE_LIMIT=100
STEAM_API_RATE_LIMIT=200

# Configurações de Socket.io (se necessário)
# SOCKET_URL=http://localhost:3000
